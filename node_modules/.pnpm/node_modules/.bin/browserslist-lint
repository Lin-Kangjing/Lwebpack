#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.4/node_modules/update-browserslist-db/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.4/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules:/mnt/f/林康景/GIT/node_modules:/mnt/f/林康景/node_modules:/mnt/f/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/node_modules/update-browserslist-db/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.4/node_modules/update-browserslist-db/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.4/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules:/mnt/f/林康景/GIT/node_modules:/mnt/f/林康景/node_modules:/mnt/f/node_modules:/mnt/f/林康景/GIT/Lwebpack/node_modules/.pnpm/node_modules/update-browserslist-db/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../update-browserslist-db/cli.js" "$@"
fi
